    <padding><Insets top="25" right="100" bottom="25" left="100"/></padding>

    <Button fx:id="greenSpacesButton" text="Green Spaces" maxWidth="Infinity"/>
    <Button fx:id="tasksButton" text="Tasks" maxWidth="Infinity"/>
    <Button fx:id="toDoListButton" text="To Do List" maxWidth="Infinity"/>
    <Button fx:id="agendaButton" text="Agenda" maxWidth="Infinity"/>
    <Button fx:id="logoutButton" text="Logout" maxWidth="Infinity"/>
    <Button fx:id="closeButton" text="Close" maxWidth="Infinity"/>
