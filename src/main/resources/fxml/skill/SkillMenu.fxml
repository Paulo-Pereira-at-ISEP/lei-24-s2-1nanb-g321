<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="pt.ipp.isep.dei.esoft.project.application.controller.fx.skill.SkillMenuFXController"
      alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml/1">
    <padding><Insets top="25" right="50" bottom="25" left="50"/></padding>

    <Button fx:id="createSkillButton" text="Create Skill" maxWidth="Infinity"/>
    <Button fx:id="listSkillsButton" text="List Skills" maxWidth="Infinity"/>
    <Button fx:id="backButton" text="Back" onAction="#handleBack" maxWidth="Infinity"/>
</VBox>