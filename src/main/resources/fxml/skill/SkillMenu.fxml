<?import javafx.scene.layout.VBox?>

<VBox fx:controller="pt.ipp.isep.dei.esoft.project.application.controller.fx.skill.SkillMenuController"
      alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml/1">
    <padding><Insets top="25" right="25" bottom="25" left="25"/></padding>

    <Button fx:id="createSkillButton" text="Create Skill"/>
    <Button fx:id="listSkillsButton" text="List Skills"/>
    <Button fx:id="backButton" text="Back" onAction="#handleBack"/>
</VBox>