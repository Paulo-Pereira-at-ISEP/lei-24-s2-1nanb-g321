<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.application.controller.fx.team.ListTeamFXController">
    <children>
        <TableView fx:id="teamsTable" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="1000.0">
            <columns>
                <TableColumn fx:id="idTeamColumn" text="ID"/>
                <TableColumn fx:id="collaboratorColumn" text="Collaborator"/>
                <TableColumn fx:id="emailColumn" text="Email"/>
                <TableColumn fx:id="jobColumn" text="Job"/>
                <TableColumn fx:id="skillsColumn" text="Skills"/>
            </columns>
        </TableView>
        <Button fx:id="backButton" layoutX="14.0" layoutY="420.0" mnemonicParsing="false" text="Back" onAction="#handleBack"/>
    </children>
</AnchorPane>